# Adicionar ao arquivo .env.local

# Google Cloud Document AI Configuration
GOOGLE_CLOUD_PROJECT_ID=your-project-id
GOOGLE_CLOUD_LOCATION=us
GOOGLE_DOCUMENT_AI_PROCESSOR_ID=your-processor-id

# Controle de Custos
GOOGLE_AI_DAILY_LIMIT=10  # Máximo 10 páginas por dia via Google AI

# Preferencial: usar base64 do JSON de serviço
GOOGLE_CREDENTIALS_BASE64=base64_encoded_service_account_json

# Alternativa: usar caminho do arquivo (local/dev)
# GOOGLE_APPLICATION_CREDENTIALS=./credentials/google-credentials.json
